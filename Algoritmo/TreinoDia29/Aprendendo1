programa {

/* OPERADORES RELACIONAIS ------
 *  
 *  == igualdade
 *  < menor que
 *  > maior que
 *  <= menor igual
 *  >= maior igual
 *  != diferente
 *  se (var) { }
 *  senao {var}
 *  senao se (var) { }



*/


		funcao inicio() {
			inteiro x = 32

		leia (x)

		se(x==32) {
			escreva("isso ai meu bom!")
			
		} senao se (x<32) {
			escreva("Ta menor em!")
		} senao {
			escreva ("Deu ruim")
		}
			
		escreva("oi")
	}
}

